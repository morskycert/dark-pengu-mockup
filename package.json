{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
    "scripts": {
    "dev": "next dev",
    "build": "next build",
    "copy:docs": "node -e \"const fs=require('fs');fs.rmSync('docs',{recursive:true,force:true});fs.mkdirSync('docs',{recursive:true});fs.cpSync('out','docs',{recursive:true});fs.writeFileSync('docs/.nojekyll','');\"",
    "deploy": "npm run build && npm run copy:docs && git add -A docs && git commit -m \"Deploy\" || echo no_changes && git push"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@gsap/react": "^2.1.2",
    "gsap": "^3.13.0",
    "lucide-react": "^0.537.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-responsive": "^10.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "shx": "^0.4.0",
    "tailwindcss": "^4"
  }
}
